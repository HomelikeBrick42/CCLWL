typedef int32 int;

distinct int yard;
distinct int meter;

typedef int (*test_func)(int x);

int fib(int x) {
    if x < 2 do return x;
    return fib(x-1) + fib(x-2);
}

int factorial(int n) {
    if n == 1 do return 1;
    return factorial(n - 1);
}

void main() {
    meter a = 1 + -2 * 3;
    yard b = 5;
    meter c = a + (meter)b;
    int i = 1;
    while i <= 10 {
        int fact = factorial(i);
        int fib = fib(i);
        i += 1;
    }
    var d = &i;
    *d = 5;
    test_func func = &fib;
    if ((int)1 == 1) do func = &factorial;
    int result = (*func)(6);
}
