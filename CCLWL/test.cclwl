typedef int32 int;

distinct int yard;
distinct int meter;

int fib(int x) {
    if x < 2 do return x;
    return fib(x-1) + fib(x-2);
}

int factorial(int n) {
    if n == 1 do return 1;
    return factorial(n - 1);
}

